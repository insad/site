{extends layout.latte}{block title}People{/block}{block body}<table class='table table-hover'>	<tr>		<th>First name</th>		<th>Last name</th>		<th>Nickname</th>		<th>Birth day</th>	</tr>    {foreach $persons as $person}        <tr>          <td>{$person['first_name']}</td>          <td>{$person['last_name']}</td>          <td>{$person['last_name']}</td>          <td>{$person['birth_day']}</td>        </tr>    {/foreach}</table>{/block}